{
  "name": "next-ts-styled-components-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "rm -rf out && next export",
    "serve": "cd out && serve -p 8080",
    "lintfix": "yarn run eslint --fix --ext .tsx,.ts,.js ./src ./pages ./stories",
    "doc": "yarn run typedoc",
    "prod": "yarn lintfix && yarn doc && yarn build && yarn export",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "next": "9.2.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.8",
    "@storybook/addon-links": "^5.3.8",
    "@storybook/addons": "^5.3.8",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^5.3.8",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.17",
    "@types/styled-components": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-react-app": "^9.1.0",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "react-is": "^16.12.0",
    "typedoc": "^0.16.7",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.7.5"
  }
}
